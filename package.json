{
  "name": "dskit-eth",
  "description": "A lightweight \"Dapp Survival Kit\" to help applications deliver fully functional experiences without sacrificing decentralization, security or privacy",
  "version": "0.1.7",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "pnpm build",
    "clean": "rm -rf dist",
    "test": "pnpm build && mocha"
  },
  "tsup": {
    "dts": true,
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ]
  },
  "dependencies": {
    "viem": "2.20.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.0",
    "@types/node": "^17.0.45",
    "chai": "^5.1.1",
    "mocha": "^10.7.3",
    "prettier": "^2.8.4",
    "tslib": "^2.6.0",
    "tsup": "^6.6.3",
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@8.12.1"
}
